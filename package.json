{
  "name": "@mck-p/helper",
  "version": "0.0.0",
  "description": "A Help Management System designed to help small groups ensure everyone is taken care of, when they need it, how they need it.",
  "main": "dist/entry.js",
  "scripts": {
    "test": "ava",
    "build": "tsc",
    "check-types": "yarn build -noEmit",
    "fmt": "prettier --write \"src/**/*.ts\"",
    "start": "node -r tsconfig-paths/register dist/entry.js",
    "dev": "nodemon --exec \"ts-node -r tsconfig-paths/register src/entry.ts\" --ext \".ts\""
  },
  "author": "Tim Roberts <tim@mck-p.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "18.7.5",
    "ava": "4.3.1",
    "nodemon": "2.0.19",
    "pino-pretty": "9.0.0",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "tsconfig-paths": "4.1.0"
  }
}
